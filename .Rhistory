analisis_aculeo <- function(anio){
disponible <- ee$ImageCollection('LANDSAT/LC08/C01/T1_TOA')$
filterDate(paste0(anio,'-11-01'),paste0(anio,'-11-30'))$
filterBounds(roi)$
filterMetadata('CLOUD_COVER','less_than', 10)
